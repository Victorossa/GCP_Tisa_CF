@model IEnumerable<GCP_CF.Models.TiposPersona>

@{
    ViewBag.Title = "Index";
}

<h2>Crear Tipo Persona</h2>

<p>
    @Html.ActionLink("Crear Nuevo", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Descripcion)

        </th>
        <th>
            @Html.DisplayNameFor(model => model.TiposNaturaleza.Descripcion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Cargo)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Descripcion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TiposNaturaleza.Descripcion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Cargo)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.TipoPersona_Id }) |
                @Html.ActionLink("Details", "Details", new { id = item.TipoPersona_Id }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.TipoPersona_Id })
            </td>
        </tr>
    }

</table>
